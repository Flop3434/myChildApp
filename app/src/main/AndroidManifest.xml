<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nutel.mychildapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity_day_view" />
        <activity
            android:name=".closet_display"
            android:label="@string/title_activity_closet_display"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".event_display"
            android:label="@string/title_activity_event_activity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".alert_display"
            android:label="@string/title_activity_alert_display"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>